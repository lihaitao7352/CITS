SELECT JTLD_TAS_YDO,JTLD_TAS_YM,JTLD_KHA_COD,JTLD_YSJ_KBN,JTLD_KMB_VER_ID,JTLD_SKS_SSI_COD,JTLD_SSI_COD,COUNT(*)FROM BGTZZ_JTLD WHERE JTLD_KHA_COD = '20512' AND (JTLD_SKS_SSI_COD IN ('TCS0154','TCS0415','TCS0416','TCS0443','TCS0444','TCS0446','TCS0447','TCS0449','TCS0450','TCS0452','TCS0453','TCS0457','TCS0458','TCS1473','TCS1474','TCS1490','TCS1495') OR JTLD_SSI_COD IN ('TCS0154','TCS0415','TCS0416','TCS0443','TCS0444','TCS0446','TCS0447','TCS0449','TCS0450','TCS0452','TCS0453','TCS0457','TCS0458','TCS1473','TCS1474','TCS1490','TCS1495')) AND ((JTLD_YSJ_KBN = 1 AND JTLD_KMB_VER_ID = 15 )OR (JTLD_YSJ_KBN = 2 AND JTLD_KMB_VER_ID = 66)
       OR (JTLD_YSJ_KBN NOT IN (1, 2))) GROUP BY JTLD_TAS_YDO,JTLD_TAS_YM,JTLD_KHA_COD,JTLD_YSJ_KBN,JTLD_KMB_VER_ID,JTLD_SKS_SSI_COD,JTLD_SSI_COD ORDER BY JTLD_TAS_YDO,JTLD_TAS_YM,JTLD_KHA_COD,JTLD_YSJ_KBN,JTLD_KMB_VER_ID,JTLD_SKS_SSI_COD,JTLD_SSI_COD;
/
SELECT JYLD_TAS_YDO,JYLD_TAS_YM,JYLD_KHA_COD,JYLD_YSJ_KBN,JYLD_KMB_VER_ID,JYLD_SKS_SSI_COD,JYLD_SSI_COD,COUNT(*)FROM BGTZZ_JYLD WHERE JYLD_KHA_COD = '20512' AND (JYLD_SKS_SSI_COD IN ('TCS0154','TCS0415','TCS0416','TCS0443','TCS0444','TCS0446','TCS0447','TCS0449','TCS0450','TCS0452','TCS0453','TCS0457','TCS0458','TCS1473','TCS1474','TCS1490','TCS1495') OR JYLD_SSI_COD IN ('TCS0154','TCS0415','TCS0416','TCS0443','TCS0444','TCS0446','TCS0447','TCS0449','TCS0450','TCS0452','TCS0453','TCS0457','TCS0458','TCS1473','TCS1474','TCS1490','TCS1495')) AND ((JYLD_YSJ_KBN = 1 AND JYLD_KMB_VER_ID = 15 )OR (JYLD_YSJ_KBN = 2 AND JYLD_KMB_VER_ID = 66)
       OR (JYLD_YSJ_KBN NOT IN (1, 2))) GROUP BY JYLD_TAS_YDO,JYLD_TAS_YM,JYLD_KHA_COD,JYLD_YSJ_KBN,JYLD_KMB_VER_ID,JYLD_SKS_SSI_COD,JYLD_SSI_COD ORDER BY JYLD_TAS_YDO,JYLD_TAS_YM,JYLD_KHA_COD,JYLD_YSJ_KBN,JYLD_KMB_VER_ID,JYLD_SKS_SSI_COD,JYLD_SSI_COD;
/
SELECT KHKD_KHA_COD,KHKD_GNK_KNR_KBN,KHKD_KMB_VER_ID,KHKD_SSI_COD,GNKD_SNA_SSI_COD,COUNT(*)FROM BGTZZ_KHKD WHERE KHKD_KHA_COD = '20512' AND (KHKD_SSI_COD IN ('TCS0154','TCS0415','TCS0416','TCS0443','TCS0444','TCS0446','TCS0447','TCS0449','TCS0450','TCS0452','TCS0453','TCS0457','TCS0458','TCS1473','TCS1474','TCS1490','TCS1495') OR KHKD_SSI_COD IN ('TCS0154','TCS0415','TCS0416','TCS0443','TCS0444','TCS0446','TCS0447','TCS0449','TCS0450','TCS0452','TCS0453','TCS0457','TCS0458','TCS1473','TCS1474','TCS1490','TCS1495')) AND ((KHKD_GNK_KNR_KBN = 1 AND KHKD_KMB_VER_ID = 15 )OR (KHKD_GNK_KNR_KBN = 2 AND KHKD_KMB_VER_ID = 66)
       OR (KHKD_GNK_KNR_KBN NOT IN (1, 2))) GROUP BY KHKD_KHA_COD,KHKD_GNK_KNR_KBN,KHKD_KMB_VER_ID,KHKD_SSI_COD,GNKD_SNA_SSI_COD ORDER BY KHKD_KHA_COD,KHKD_GNK_KNR_KBN,KHKD_KMB_VER_ID,KHKD_SSI_COD,GNKD_SNA_SSI_COD;
