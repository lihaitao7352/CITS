       OR (KTLD_YSJ_KBN NOT IN (1, 2))) GROUP BY KTLD_TAS_YDO,KTLD_TAS_YM,KTLD_KHA_COD,KTLD_YSJ_KBN,KTLD_KMB_VER_ID,KTLD_HFM_SSI_COD,KTLD_HFS_SSI_COD ORDER BY KTLD_TAS_YDO,KTLD_TAS_YM,KTLD_KHA_COD,KTLD_YSJ_KBN,KTLD_KMB_VER_ID,KTLD_HFM_SSI_COD,KTLD_HFS_SSI_COD;
