       OR (KHKE_GNK_KNR_KBN NOT IN (1, 2))) GROUP BY KHKE_KHA_COD,KHKE_GNK_KNR_KBN,KHKE_KMB_VER_ID,KHKE_SSI_COD,GNKD_SNA_SSI_COD ORDER BY KHKE_KHA_COD,KHKE_GNK_KNR_KBN,KHKE_KMB_VER_ID,KHKE_SSI_COD,GNKD_SNA_SSI_COD;
